{% extends 'base.html.twig' %}


{% block body %}
    <h2>Modifier compte</h2>
    {# <form method="post"> #}
        {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.prenom) }}
        {{ form_row(form.email) }}
        {{ form_row(form.adresse) }}
        {{ form_row(form.ville) }}
        {{ form_row(form.postalcode) }}
        <button type="submit" class="btn btn-primary">Save Changes</button>
        {{ form_end(form) }}
    </form>
{% endblock %}

