{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!{% endblock %}


{% block body %}
    <h2>Nous contacter</h2>
    {# <form method="post"> #}
        {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.prenom) }}
        {{ form_row(form.email) }}
        {{ form_row(form.sujet) }}
        {{ form_row(form.message) }}
        <button type="submit" class="btn btn-primary">Save Changes</button>
        {{ form_end(form) }}
    </form>
{% endblock %}

